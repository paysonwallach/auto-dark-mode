{
    "name": "mojave-dark-mode",
    "main": "./dist/main",
    "version": "0.2.3",
    "author": "Payson Wallach",
    "description": "Dark Mode support for macOS Mojave.",
    "keywords": [
        "dark mode",
        "themes",
        "switch",
        "macOS",
        "atom"
    ],
    "activationCommands": {},
    "repository": "https://github.com/paysonwallach/mojave-dark-mode",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "configSchema": {
        "darkProfile": {
            "order": 1,
            "description": "UI and syntax theme for dark mode",
            "type": "string",
            "default": "one-dark-ui one-dark-syntax"
        },
        "lightProfile": {
            "order": 2,
            "description": "UI and syntax theme for light mode",
            "type": "string",
            "default": "one-light-ui one-light-syntax"
        }
    },
    "dependencies": {},
    "devDependencies": {
        "@types/atom": "^1.31.0",
        "typescript": "^3.1.2"
    }
}
